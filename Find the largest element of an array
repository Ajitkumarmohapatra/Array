 class largest_element_of_an_array {
    
    public static void main(String[] args) {
        int num[]={25, 11, 7, 75, 56};
        int max=num[0];
        
        for(int i=0;i<num.length;i++){
            if(num[i]>max){
                max=num[i];
            }
        }
        System.out.println("Largest element of an array is :");
        System.out.println(max);
    }
}
